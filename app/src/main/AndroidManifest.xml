<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Mpdam.Androggek.MaterialDesignSample">

    <application
        android:name="com.Mpdam.Androggek.MaterialDesignSample.App"
        android:allowBackup="true"
        android:icon="@drawable/isetrgg"
        android:label="@string/app_name"

        android:theme="@style/AppTheme">

        <activity
            android:name="com.Mpdam.Androggek.MaterialDesignSample.activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
